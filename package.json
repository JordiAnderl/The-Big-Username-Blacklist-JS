    "test": "mocha --reporter spec test",
    "hint": "jshint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marteinn/The-Big-Username-Blacklist-JS.git"
  },
  "keywords": [
    "username",
    "blacklist"
  ],
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel": "6.5.2",
    "jshint": "2.9.4",
    "mocha": "3.1.2"
  },
